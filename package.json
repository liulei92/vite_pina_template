    "windicss": "^3.5.6"
  },
  "engines": {
    "node": ">= 12.18",
    "npm": ">= 6.14.4"
  },
  "pre-commit": [
    "lint:eslint"
  ],
  "vite": {
    "optimizeDeps": {
      "include": [
