{
  "name": "vite_pina_demo",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "check": "cross-env vue-tsc --noEmit",
    "dev": "cross-env vite",
    "devs": "cross-env HTTPS=true vite",
    "build": "cross-env vite build",
    "build:check": "cross-env vue-tsc --noEmit && vite build",
    "preview": "cross-env vite preview",
    "clean:cache": "cross-env rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "cross-env rimraf node_modules",
    "lint:eslint": "cross-env eslint --cache --max-warnings 0 \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "cross-env prettier --write \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\""
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@ckpack/vue-color": "^1.1.8",
    "@vueuse/core": "^8.1.2",
    "ant-design-vue": "^3.2.0",
    "axios": "^0.26.1",
    "dayjs": "^1.11.0",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.12",
    "vue": "^3.2.25",
    "vue-i18n": "^9.1.9",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vitejs/plugin-legacy": "^1.7.1",
    "@vitejs/plugin-vue": "^2.2.4",
    "@vitejs/plugin-vue-jsx": "^1.3.8",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.20.0",
    "less": "^4.1.2",
    "mockjs": "^1.1.0",
    "postcss-preset-env": "^7.4.3",
    "postcss-sass-unicode": "^0.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.6.0",
    "rimraf": "^3.0.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "sass": "^1.49.9",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.6.6",
    "unplugin-vue-components": "^0.18.5",
    "vite": "^2.8.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-optimize-persist": "^0.1.2",
    "vite-plugin-package-config": "^0.1.1",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vite-plugin-windicss": "^1.8.3",
    "vue-eslint-parser": "^8.3.0",
    "vue-tsc": "^0.29.8",
    "windicss": "^3.5.1"
  },
  "engines": {
    "node": ">= 12.18",
    "npm": ">= 6.14.4"
  },
  "pre-commit": [
    "lint:eslint"
  ],
  "vite": {
    "optimizeDeps": {
      "include": [
        "@ant-design/icons-vue",
        "@ckpack/vue-color",
        "@vueuse/core",
        "ant-design-vue",
        "ant-design-vue/es",
        "ant-design-vue/es/button/buttonTypes",
        "ant-design-vue/es/button/style",
        "ant-design-vue/es/checkbox/interface",
        "ant-design-vue/es/checkbox/style",
        "ant-design-vue/es/col/style",
        "ant-design-vue/es/config-provider/style",
        "ant-design-vue/es/date-picker/style",
        "ant-design-vue/es/divider/style",
        "ant-design-vue/es/dropdown/style",
        "ant-design-vue/es/form",
        "ant-design-vue/es/form/FormItem",
        "ant-design-vue/es/form/style",
        "ant-design-vue/es/input-number/style",
        "ant-design-vue/es/input/inputProps",
        "ant-design-vue/es/input/style",
        "ant-design-vue/es/menu/style",
        "ant-design-vue/es/popover/style",
        "ant-design-vue/es/radio/Group",
        "ant-design-vue/es/radio/style",
        "ant-design-vue/es/row/style",
        "ant-design-vue/es/select",
        "ant-design-vue/es/select/style",
        "ant-design-vue/es/space/style",
        "ant-design-vue/es/spin/style",
        "ant-design-vue/es/switch",
        "ant-design-vue/es/switch/style",
        "ant-design-vue/es/time-picker/style",
        "ant-design-vue/es/tooltip/style",
        "ant-design-vue/es/upload/interface",
        "ant-design-vue/es/upload/style",
        "ant-design-vue/lib/_util/vue-types",
        "ant-design-vue/lib/checkbox/interface",
        "ant-design-vue/lib/form/FormItem",
        "ant-design-vue/lib/input/inputProps",
        "ant-design-vue/lib/radio/Group",
        "ant-design-vue/lib/select",
        "ant-design-vue/lib/switch",
        "ant-design-vue/lib/upload/interface",
        "axios",
        "dayjs",
        "lodash-es",
        "pinia",
        "vue",
        "vue-i18n",
        "vue-router"
      ]
    }
  }
}
