    "sass": "^1.53.0",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.9.3",
    "unplugin-vue-components": "^0.21.1",
    "vite": "^3.0.0",
    "vite-plugin-compression": "^0.5.1",
